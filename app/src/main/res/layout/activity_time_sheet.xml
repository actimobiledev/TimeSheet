<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout
    android:id="@+id/clMain" xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff">

    <android.support.design.widget.AppBarLayout
        android:id="@+id/appBar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:animateLayoutChanges="true"
        android:theme="@style/AppTheme.AppBarOverlay"
        android:visibility="visible"
        app:elevation="0dp">
        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar1"
            style="@style/Widget.MyApp.ActionBar"
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:animateLayoutChanges="true"
            android:background="#ffffff"
            android:theme="@style/Base.ThemeOverlay.AppCompat.Dark"
            app:contentInsetEnd="0dp"
            app:contentInsetLeft="0dp"
            app:contentInsetRight="0dp"
            app:contentInsetStart="0dp"
            app:layout_collapseMode="parallax">

            <RelativeLayout
                android:id="@+id/toolbar_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <RelativeLayout
                    android:id="@+id/rlBack"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true">
                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_centerVertical="true"
                        android:layout_margin="@dimen/activity_margin"
                        android:src="@drawable/ic_arrow_back"
                        android:textColor="@color/text_color_grey_dark2"
                        android:visibility="visible"/>
                </RelativeLayout>
                <TextView
                    android:id="@+id/tvTitleEventName"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_centerInParent="true"
                    android:ellipsize="end"
                    android:lines="1"
                    android:maxLines="1"
                    android:text="TIMESHEET"
                    android:textColor="@color/primary_text"
                    android:textSize="@dimen/text_size_medium"
                    android:textStyle="bold"/>

                <ImageView
                    android:id="@+id/ivSave"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:padding="@dimen/activity_margin"
                    android:src="@drawable/ic_save"
                    android:visibility="visible"/>
            </RelativeLayout>
        </android.support.v7.widget.Toolbar>
        <View
            android:id="@+id/v1"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_alignParentBottom="true"
            android:background="@color/text_color_grey_light2"
            android:visibility="visible"/>
    </android.support.design.widget.AppBarLayout>
    <android.support.v4.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/appBar"
        android:descendantFocusability="blocksDescendants"
        android:fitsSystemWindows="true"
        android:overScrollMode="never"
        app:layout_behavior="@string/appbar_scrolling_view_behavior">
        <RelativeLayout
            android:id="@+id/rlList"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/appBar"
            android:visibility="visible">
            <RelativeLayout
                android:id="@+id/llProjectName"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/activity_margin"
                android:background="@drawable/edit_background"
                android:orientation="horizontal"
                android:weightSum="5">

                <TextView
                    android:id="@+id/tvProjectName"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toLeftOf="@+id/iv1"
                    android:layout_toStartOf="@+id/iv1"
                    android:gravity="center"
                    android:padding="@dimen/activity_margin_small"
                    android:text="Project name"
                    android:textColor="@color/primary_text"
                    android:textSize="@dimen/text_size_medium"/>
                <ImageView
                    android:id="@+id/iv1"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:padding="@dimen/activity_margin_small"
                    android:src="@drawable/ic_arrow_down"/>
            </RelativeLayout>
            <LinearLayout
                android:id="@+id/llTitle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/llProjectName"
                android:layout_marginLeft="@dimen/activity_margin"
                android:layout_marginRight="@dimen/activity_margin"
                android:background="@drawable/bg_question"
                android:orientation="horizontal"
                android:weightSum="5">
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2.5"
                    android:background="@drawable/bg_question"
                    android:orientation="vertical"
                    android:padding="@dimen/activity_margin_small">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="Day / Date"
                        android:textColor="@color/primary_text"
                        android:textSize="@dimen/text_size_small"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@drawable/bg_question"
                    android:orientation="vertical"
                    android:padding="@dimen/activity_margin_small">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="Hours"
                        android:textColor="@color/primary_text"
                        android:textSize="@dimen/text_size_small"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:background="@drawable/bg_question"
                    android:orientation="vertical"
                    android:padding="@dimen/activity_margin_small">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="*"
                        android:textColor="@color/primary_text"
                        android:textSize="@dimen/text_size_small"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@drawable/bg_question"
                    android:orientation="vertical"
                    android:padding="@dimen/activity_margin_small">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="Total"
                        android:textColor="@color/primary_text"
                        android:textSize="@dimen/text_size_small"/>


                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/llDay1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/llTitle"
                android:layout_marginLeft="@dimen/activity_margin"
                android:layout_marginRight="@dimen/activity_margin"
                android:background="@drawable/bg_question"
                android:orientation="horizontal"
                android:weightSum="5">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2.5"
                    android:background="@drawable/bg_question"
                    android:orientation="vertical"
                    android:padding="@dimen/activity_margin_small">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="Monday"
                        android:textColor="@color/primary_text"
                        android:textSize="@dimen/text_size_small"/>
                    <TextView
                        android:id="@+id/tvDate1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text=""
                        android:textColor="@color/accent"
                        android:textSize="@dimen/text_size_extra_small"/>
                </LinearLayout>
                <EditText
                    android:id="@+id/etMondayhour"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/bg_question"
                    android:gravity="center"
                    android:inputType="numberDecimal"
                    android:text=""
                    android:textColor="@color/primary_text"
                    android:textSize="@dimen/text_size_small"
                    android:textStyle="bold"/>
                <ImageView
                    android:id="@+id/ivMondayComment"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.5"
                    android:background="@drawable/bg_question"
                    android:padding="@dimen/activity_margin_small"
                    android:src="@drawable/ic_comment_2"/>
                <TextView
                    android:id="@+id/tvDate1Total"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/bg_question"
                    android:gravity="center"
                    android:text="0"
                    android:textColor="@color/primary_text"
                    android:textSize="@dimen/text_size_small"
                    android:textStyle="bold"
                />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/llDay2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/llDay1"
                android:layout_marginLeft="@dimen/activity_margin"
                android:layout_marginRight="@dimen/activity_margin"
                android:background="@drawable/bg_question"
                android:orientation="horizontal"
                android:weightSum="5">
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2.5"
                    android:background="@drawable/bg_question"
                    android:orientation="vertical"
                    android:padding="@dimen/activity_margin_small">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="Tuesday"
                        android:textColor="@color/primary_text"
                        android:textSize="@dimen/text_size_small"/>

                    <TextView
                        android:id="@+id/tvDate2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text=""
                        android:textColor="@color/accent"
                        android:textSize="@dimen/text_size_extra_small"/>


                </LinearLayout>
                <EditText
                    android:id="@+id/etTueshour"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/bg_question"
                    android:gravity="center"
                    android:inputType="numberDecimal"
                    android:textColor="@color/primary_text"
                    android:textSize="@dimen/text_size_small"
                    android:textStyle="bold"/>
                <ImageView
                    android:id="@+id/ivTuesdayComment"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.5"
                    android:background="@drawable/bg_question"
                    android:padding="@dimen/activity_margin_small"
                    android:src="@drawable/ic_comment_2"/>
                <TextView
                    android:id="@+id/tvDate2Total"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/bg_question"
                    android:gravity="center"
                    android:text="0"
                    android:textColor="@color/primary_text"
                    android:textSize="@dimen/text_size_small"
                    android:textStyle="bold"
                />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/llDay3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/llDay2"
                android:layout_marginLeft="@dimen/activity_margin"
                android:layout_marginRight="@dimen/activity_margin"
                android:background="@drawable/bg_question"
                android:orientation="horizontal"
                android:weightSum="5">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2.5"
                    android:background="@drawable/bg_question"
                    android:orientation="vertical"
                    android:padding="@dimen/activity_margin_small">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="Wednesday"
                        android:textColor="@color/primary_text"
                        android:textSize="@dimen/text_size_small"/>

                    <TextView
                        android:id="@+id/tvDate3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text=""
                        android:textColor="@color/accent"
                        android:textSize="@dimen/text_size_extra_small"/>


                </LinearLayout>

                <EditText
                    android:id="@+id/etWednesdayHour"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/bg_question"
                    android:gravity="center"
                    android:inputType="numberDecimal"
                    android:textColor="@color/primary_text"
                    android:textSize="@dimen/text_size_small"
                    android:textStyle="bold"/>
                <ImageView
                    android:id="@+id/ivWednesdayComment"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.5"
                    android:background="@drawable/bg_question"
                    android:padding="@dimen/activity_margin_small"
                    android:src="@drawable/ic_comment_2"/>
                <TextView
                    android:id="@+id/tvDate3Total"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/bg_question"
                    android:gravity="center"
                    android:text="0"
                    android:textColor="@color/primary_text"
                    android:textSize="@dimen/text_size_small"
                    android:textStyle="bold"
                />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/llDay4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/llDay3"
                android:layout_marginLeft="@dimen/activity_margin"
                android:layout_marginRight="@dimen/activity_margin"
                android:background="@drawable/bg_question"
                android:orientation="horizontal"
                android:weightSum="5">
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2.5"
                    android:background="@drawable/bg_question"
                    android:orientation="vertical"
                    android:padding="@dimen/activity_margin_small">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="Thursday"
                        android:textColor="@color/primary_text"
                        android:textSize="@dimen/text_size_small"/>

                    <TextView
                        android:id="@+id/tvDate4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text=""
                        android:textColor="@color/accent"
                        android:textSize="@dimen/text_size_extra_small"/>


                </LinearLayout>
                <EditText
                    android:id="@+id/etThursdayhour"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/bg_question"
                    android:gravity="center"
                    android:inputType="numberDecimal"
                    android:textColor="@color/primary_text"
                    android:textSize="@dimen/text_size_small"
                    android:textStyle="bold"/>
                <ImageView
                    android:id="@+id/ivThursdayComment"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.5"
                    android:background="@drawable/bg_question"
                    android:padding="@dimen/activity_margin_small"
                    android:src="@drawable/ic_comment_2"/>
                <TextView
                    android:id="@+id/tvDate4Total"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/bg_question"
                    android:gravity="center"
                    android:text="0"
                    android:textColor="@color/primary_text"
                    android:textSize="@dimen/text_size_small"
                    android:textStyle="bold"
                />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/llDay5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/llDay4"
                android:layout_marginLeft="@dimen/activity_margin"
                android:layout_marginRight="@dimen/activity_margin"
                android:background="@drawable/bg_question"
                android:orientation="horizontal"
                android:weightSum="5">
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2.5"
                    android:background="@drawable/bg_question"
                    android:orientation="vertical"
                    android:padding="@dimen/activity_margin_small">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="Friday"
                        android:textColor="@color/primary_text"
                        android:textSize="@dimen/text_size_small"/>

                    <TextView
                        android:id="@+id/tvDate5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text=""
                        android:textColor="@color/accent"
                        android:textSize="@dimen/text_size_extra_small"/>
                </LinearLayout>
                <EditText
                    android:id="@+id/etFridayhour"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/bg_question"
                    android:gravity="center"
                    android:inputType="numberDecimal"
                    android:textColor="@color/primary_text"
                    android:textSize="@dimen/text_size_small"
                    android:textStyle="bold"/>
                <ImageView
                    android:id="@+id/ivFridayComment"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.5"
                    android:background="@drawable/bg_question"
                    android:padding="@dimen/activity_margin_small"
                    android:src="@drawable/ic_comment_2"/>
                <TextView
                    android:id="@+id/tvDate5Total"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/bg_question"
                    android:gravity="center"
                    android:text="0"
                    android:textColor="@color/primary_text"
                    android:textSize="@dimen/text_size_small"
                    android:textStyle="bold"
                />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/llDay6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/llDay5"
                android:layout_marginLeft="@dimen/activity_margin"
                android:layout_marginRight="@dimen/activity_margin"
                android:background="@drawable/bg_question"
                android:orientation="horizontal"
                android:weightSum="5">
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2.5"
                    android:background="@drawable/bg_question"
                    android:orientation="vertical"
                    android:padding="@dimen/activity_margin_small">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="Saturday"
                        android:textColor="@color/primary_text"
                        android:textSize="@dimen/text_size_small"/>

                    <TextView
                        android:id="@+id/tvDate6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text=""
                        android:textColor="@color/accent"
                        android:textSize="@dimen/text_size_extra_small"/>


                </LinearLayout>
                <EditText
                    android:id="@+id/etSaturdayhour"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/bg_question"
                    android:gravity="center"
                    android:inputType="numberDecimal"
                    android:textColor="@color/primary_text"
                    android:textSize="@dimen/text_size_small"
                    android:textStyle="bold"/>
                <ImageView
                    android:id="@+id/ivSaturdayComment"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.5"
                    android:background="@drawable/bg_question"
                    android:padding="@dimen/activity_margin_small"
                    android:src="@drawable/ic_comment_2"/>
                <TextView
                    android:id="@+id/tvDate6Total"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/bg_question"
                    android:gravity="center"
                    android:text="0"
                    android:textColor="@color/primary_text"
                    android:textSize="@dimen/text_size_small"
                    android:textStyle="bold"
                />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/llDay7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/llDay6"
                android:layout_marginLeft="@dimen/activity_margin"
                android:layout_marginRight="@dimen/activity_margin"
                android:background="@drawable/bg_question"
                android:orientation="horizontal"
                android:weightSum="5">
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2.5"
                    android:background="@drawable/bg_question"
                    android:orientation="vertical"
                    android:padding="@dimen/activity_margin_small">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="Sunday"
                        android:textColor="@color/primary_text"
                        android:textSize="@dimen/text_size_small"/>

                    <TextView
                        android:id="@+id/tvDate7"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text=""
                        android:textColor="@color/accent"
                        android:textSize="@dimen/text_size_extra_small"/>


                </LinearLayout>
                <EditText
                    android:id="@+id/etSundayhour"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/bg_question"
                    android:gravity="center"
                    android:inputType="numberDecimal"
                    android:textColor="@color/primary_text"
                    android:textSize="@dimen/text_size_small"
                    android:textStyle="bold"/>
                <ImageView
                    android:id="@+id/ivSundayComment"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.5"
                    android:background="@drawable/bg_question"
                    android:padding="@dimen/activity_margin_small"
                    android:src="@drawable/ic_comment_2"/>
                <TextView
                    android:id="@+id/tvDate7Total"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/bg_question"
                    android:gravity="center"
                    android:text="0"
                    android:textColor="@color/primary_text"
                    android:textSize="@dimen/text_size_small"
                    android:textStyle="bold"
                />

            </LinearLayout>
            <RelativeLayout
                android:id="@+id/llPrevious"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/llDay7"
                android:layout_marginBottom="@dimen/activity_margin_extra_large"
                android:layout_marginLeft="@dimen/activity_margin"
                android:layout_marginRight="@dimen/activity_margin"
                android:layout_marginTop="@dimen/activity_margin_extra_large"
                android:background="@drawable/bg_question"
            >
                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_centerVertical="true"
                    android:layout_margin="@dimen/activity_margin"
                    android:src="@drawable/ic_double_left"
                />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
